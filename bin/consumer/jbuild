(jbuild_version 1)

(executables
 ((names (event_logger))
  (package datamint)
  (public_names (datamint-event-logger))
  (preprocess (pps (ppx_sexp_conv ppx_driver.runner)))
  (libraries (datamint cmdliner sexplib cohttp-lwt-unix websocket-lwt))))

(install
  ((section bin)
   (package datamint)
   (files (
     (event-logger.conf as datamint-event-logger.conf)
   ))))
